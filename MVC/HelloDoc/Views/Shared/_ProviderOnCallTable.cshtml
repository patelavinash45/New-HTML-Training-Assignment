@model Services.ViewModels.Admin.ProviderList

<h5 class="mt-3">
    MDs On Call
</h5>
<div class="row">
    @foreach (var item in Model.ProviderOnCall)
    {
        <div class="col-xl-3 col-lg-4 col-md-6 mt-3">
            <img src="@item.Photo" class="profilePhoto" />
            @item.FirstName @item.LastName
        </div>
    }
</div>
<h5 class="mt-3">
    Provider Off Duty
</h5>
<div class="row">
    @foreach (var item in Model.ProviderOffDuty)
    {
        <div class="col-xl-3 col-lg-4 col-md-6 mt-3">
            <img src="@item.Photo" class="profilePhoto" />
            @item.FirstName @item.LastName
        </div>
    }
</div>
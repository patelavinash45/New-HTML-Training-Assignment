<!DOCTYPE html>
<html lang="en">
<head>
    <title>
        @ViewData["Title"]
    </title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <link href="~/css/international-telephone-input.css" rel="stylesheet" />
    <script src="~/js/international-telephone-input.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" />
    <script type="text/javascript" src="@ViewBag.JsFile"></script>
    <link rel="stylesheet" href="@ViewBag.CssFile" />
</head>
<body>
    @await Component.InvokeAsync("Notyf")
    @if (ViewBag.Header != null)
    {
        <partial name="@ViewBag.Header" />
    }
    @RenderBody()
</body>
</html>
